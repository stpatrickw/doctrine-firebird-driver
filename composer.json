{
    "name": "stpatrickw/doctrine-firebird-driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "St Patrick"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kafoso\\DoctrineFirebirdDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Resource\\": "tests/resources/Test/Resource",
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Integration\\": "tests/tests/Test/Integration",
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Unit\\": "tests/tests/Test/Unit"
        }
    },
    "require": {
        "doctrine/dbal": ">=2.5 <=2.5.13|^3.1.*",
        "php": ">=7.1"
    },
    "require-dev": {
        "doctrine/orm": "2.5.*|^2.10",
        "phpunit/phpunit": "^5.7"
    }
}
